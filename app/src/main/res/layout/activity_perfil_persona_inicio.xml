<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/emailPerfil"
        android:layout_width="141dp"
        android:layout_height="41dp"
        android:layout_marginTop="48dp"
        android:text="Nombre"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/apePerfil"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/apePerfil"
        app:layout_constraintTop_toBottomOf="@+id/apePerfil" />

    <TextView
        android:id="@+id/nombrePerfil"
        android:layout_width="141dp"
        android:layout_height="41dp"
        android:layout_marginTop="48dp"
        android:text="Nombre"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.827"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/txtTexto4"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/apePerfil"
        android:layout_width="141dp"
        android:layout_height="41dp"
        android:layout_marginTop="48dp"
        android:text="Nombre"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/nombrePerfil"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nombrePerfil"
        app:layout_constraintTop_toBottomOf="@+id/nombrePerfil" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="141dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="48dp"
        android:text="Email"
        android:textColor="#003C6E"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/txtTexto4"
        android:layout_width="158dp"
        android:layout_height="68dp"
        android:gravity="center"
        android:text="Tu Perfil"
        android:textColor="#003C6E"
        android:textStyle="bold"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="141dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="48dp"
        android:text="Nombre"
        android:textColor="#003C6E"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTexto4" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="141dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="48dp"
        android:text="Apellidos"
        android:textColor="#003C6E"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />
</androidx.constraintlayout.widget.ConstraintLayout>